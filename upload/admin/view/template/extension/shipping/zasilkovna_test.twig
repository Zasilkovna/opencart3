{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <h1>Test</h1>
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                {% endfor %}
            </ul>
            <div class="pull-right" style="margin-top: -5px;">
                <button type="submit" form="form-add-vendor" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button>
                <a href="{{ action_back }}" data-toggle="tooltip" title="{{ text_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i> </a>
            </div>
        </div>
    </div>
    <div class="container-fluid">
        {% if flashMessage %}
            <div class="alert {{ flashMessage.class }} alert-dismissible"><i class="fa {{ flashMessage.icon }}"></i> {{ flashMessage.text }}
                <button type="button" class="close" data-dismiss="alert">&times;</button>
            </div>
        {% endif %}

    </div>

        {% for key, debug in debugs %}
            {{ key }}
            <pre style="margin-left:20px">
            {{ debug }}
            </pre>
        {% endfor %}

</div>


{{ footer }}
